{% extends 'default_frame.twig' %}

{% set body_class = 'front_page' %}

{% block javascript %}
    <script>
        $(function() {
            $('#mainVisual .sliderFor').slick({
                slidesToShow: 1,
                slidesToScroll: 1,
                arrows: false,
                fade: true,
                autoplay: true,
                asNavFor: '.sliderNav',
                responsive: [
                    {
                        breakpoint: 768,
                        settings: {
                            arrows: true,
                            dots: true
                        }
                    }
                ]
            });
            $('#mainVisual .sliderNav').slick({
                slidesToShow: 6,
                slidesToScroll: 1,
                asNavFor: '.sliderFor',
                arrows: true,
                focusOnSelect: true
            });
        });
    </script>
{% endblock %}

{% block main %}
    <input type="hidden" id="pageID" value="gNav01">
    <div id="mainVisual">
